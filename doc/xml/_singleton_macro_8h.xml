        <name>GENERATE_SINGLETON_IMPLEMENT</name>
        <param><defname>CLASS_NAME</defname></param>
        <initializer>CLASS_NAME *CLASS_NAME::instance = NULL;\
CLASS_NAME *CLASS_NAME::getInstance() {\
